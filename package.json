{
  "name": "global-holidays-service",
  "version": "0.0.1",
  "description": "",
  "author": "William Koller",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:coveralls": "npm run -- test:ci && coveralls < coverage/lcov.info",
    "test:staged": "npm test -- --findRelatedTests",
    "check": "npm-check -s -u",
    "typeorm:drop": "npm run typeorm schema:drop",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/ormconfig.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "npm run typeorm -- migration:create -n",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:show": "npm run typeorm -- migration:show",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "seed:create": "npm run typeorm -- migration:create -c seed -n ",
    "seed:run": "npm run typeorm -- migration:run -c seed",
    "seed:revert": "npm run typeorm -- migration:revert -c seed",
    "start:dev:db-all": "./src/scripts/start-db.sh && npm run typeorm:migration:run && npm run seed:run"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/typeorm": "^7.1.5",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.2.6",
    "lodash": "^4.17.20",
    "npm-check": "^5.9.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.5",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.28",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
